{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "d": {
      "dependsOn": [
        "^d"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true,
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "{projectRoot}/out"
      ],
      "cache": true
    }
  },
  "defaultBase": "main",
  "parallel": 8,
  "nxCloudAccessToken": "YzBhOTcxNjktYjUyZi00M2I0LWI2N2MtZGJkZTkzMDZkYmIxfHJlYWQtd3JpdGU="
}