{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    }
  },
  "parallel": 5,
  "defaultBase": "main",
  "plugins": [
  {
      "plugin": "@nx/rollup/plugin",
      "options": {
        "buildTargetName": "rollup:build"
      }
    }
  ]
}
