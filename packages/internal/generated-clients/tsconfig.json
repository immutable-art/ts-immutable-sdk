{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "../../../sdk/dist/esm/generated-clients",
    "rootDirs": ["src"],
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "node",
    // "noEmit": true,
    "allowJs": false,
    "removeComments": false,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "composite": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
